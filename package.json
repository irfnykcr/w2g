{
	"name": "w2g",
	"version": "0.2.2",
	"description": "",
	"license": "ISC",
	"author": "",
	"type": "commonjs",
	"main": "index.js",
	"scripts": {
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"devDependencies": {
		"electron": "^36.3.2",
		"electron-builder": "^26.0.12"
	},
	"dependencies": {
		"@distube/ytdl-core": "^4.16.12",
		"axios": "^1.9.0",
		"bcryptjs": "^3.0.2",
		"keytar": "^7.9.0",
		"ws": "^8.18.3",
		"youtube-dl-exec": "^3.0.24"
	},
	"build": {
		"appId": "com.turkuazz.w2g",
		"productName": "TURKUAZZ",
		"linux": {
			"target": [
				"AppImage"
			],
			"category": "Utility"
		},
		"win": {
			"target": [
				"nsis"
			],
			"icon": "winicon.png"
		},
		"nsis": {
			"createStartMenuShortcut": false,
			"oneClick": false,
			"runAfterFinish": false,
			"createDesktopShortcut": true,
			"allowToChangeInstallationDirectory": true
		},
		"files": [
			"**/*",
			"!test_server/**/*"
		],
		"extraResources": [
			{
				"from": "resources/config",
				"to": "config"
			}
		]
	}
}
