{
	"name": "w2g",
	"version": "1.0.0",
	"description": "",
	"license": "ISC",
	"author": "",
	"type": "commonjs",
	"main": "index.js",
	"scripts": {
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"devDependencies": {
		"electron": "^36.3.2",
		"electron-builder": "^26.0.12"
	},
	"dependencies": {
		"axios": "^1.9.0",
		"bcryptjs": "^3.0.2"
	},
	"build": {
		"appId": "com.turkuazz.w2g",
		"productName": "TURKUAZZ",
		"linux": {
			"target": [
				"AppImage"
			],
			"category": "Utility"
		},
		"win": {
			"target": [
				"nsis"
			],
			"icon": "winicon.png"
		},
		"files": [
			"**/*",
			"!test_server/**/*"
		],
		"extraResources": [
			{
				"from": "resources/config",
				"to": "config"
			},
			{
				"from": "test_server/server.py",
				"to": "test_server/server.py"
			}
		]
	}
}
